include/assert.inc [the result of evaluating 'mask_ssn('123-45-6789', 'Ю')' must be equal to 'ЮЮЮ-ЮЮ-6789']
include/assert.inc [charset of the result of evaluating 'mask_ssn('123-45-6789', 'Ю')' must be 'utf8mb4']
include/assert.inc [collation of the result of evaluating 'mask_ssn('123-45-6789', 'Ю')' must be 'utf8mb4_0900_ai_ci']
SET @check_expression_result = mask_ssn(_ascii '123-45-6789', 'Ю');
include/assert.inc [the result of evaluating 'mask_ssn(_ascii '123-45-6789', 'Ю')' must be equal to '???-??-6789']
include/assert.inc [charset of the result of evaluating 'mask_ssn(_ascii '123-45-6789', 'Ю')' must be 'ascii']
include/assert.inc [collation of the result of evaluating 'mask_ssn(_ascii '123-45-6789', 'Ю')' must be 'ascii_general_ci']
# character set / collation checks
SET @regular_charset_list = '[ "utf8mb4", "utf8mb3", "utf16", "utf16le", "utf32", "ucs2", "koi8u"]';
SET @special_charset_list = '[ "latin2", "ascii", "binary"]';
SET @charset_list = JSON_MERGE_PRESERVE(@regular_charset_list, @special_charset_list);
