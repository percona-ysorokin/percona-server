#
# Component Masking Functions: mask_ssn()
#
INSTALL COMPONENT 'file://component_masking_functions';
# arity checks
SELECT mask_ssn();
ERROR HY000: Can't initialize function 'mask_ssn'; Wrong argument list: should be (string, [char])
SELECT mask_ssn('', '', '');
ERROR HY000: Can't initialize function 'mask_ssn'; Wrong argument list: should be (string, [char])
# argument nullness checks
SELECT mask_ssn('', NULL);
ERROR HY000: mask_ssn<string> UDF failed; argument 2 cannot be null
# constraints checks
SELECT mask_ssn('');
ERROR HY000: mask_ssn<string> UDF failed; Argument must be between 9 and 11 characters
SELECT mask_ssn('12345678');
ERROR HY000: mask_ssn<string> UDF failed; Argument must be between 9 and 11 characters
SELECT mask_ssn('123456789012');
ERROR HY000: mask_ssn<string> UDF failed; Argument must be between 9 and 11 characters
SELECT mask_ssn('123456789', '');
ERROR HY000: mask_ssn<string> UDF failed; masking character must be of length 1
SELECT mask_ssn('123456789', 'XX');
ERROR HY000: mask_ssn<string> UDF failed; masking character must be of length 1
# NULL for NULL checks
include/assert.inc [mask_ssn() for the NULL primary argument should return NULL]
include/assert.inc [mask_ssn() for the NULL primary argument and custom mask should return NULL]
# logic checks
SET @check_expression_result = mask_ssn('123456789');
include/assert.inc [the result of evaluating 'mask_ssn('123456789')' must be equal to '*****6789']
include/assert.inc [charset of the result of evaluating 'mask_ssn('123456789')' must be 'utf8mb4']
include/assert.inc [collation of the result of evaluating 'mask_ssn('123456789')' must be 'utf8mb4_0900_ai_ci']
SET @check_expression_result = mask_ssn('123-45-6789');
include/assert.inc [the result of evaluating 'mask_ssn('123-45-6789')' must be equal to '***-**-6789']
include/assert.inc [charset of the result of evaluating 'mask_ssn('123-45-6789')' must be 'utf8mb4']
include/assert.inc [collation of the result of evaluating 'mask_ssn('123-45-6789')' must be 'utf8mb4_0900_ai_ci']
