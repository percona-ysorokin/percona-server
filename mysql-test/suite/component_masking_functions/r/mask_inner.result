#
# Component Masking Functions: mask_inner()
#
INSTALL COMPONENT 'file://component_masking_functions';
# arity checks
SELECT mask_inner();
ERROR HY000: Can't initialize function 'mask_inner'; Wrong argument list: should be (string, int, int, [char])
SELECT mask_inner('');
ERROR HY000: Can't initialize function 'mask_inner'; Wrong argument list: should be (string, int, int, [char])
SELECT mask_inner('', 42);
ERROR HY000: Can't initialize function 'mask_inner'; Wrong argument list: should be (string, int, int, [char])
SELECT mask_inner('', 42, 42, '', '');
ERROR HY000: Can't initialize function 'mask_inner'; Wrong argument list: should be (string, int, int, [char])
# argument nullness checks
SELECT mask_inner('', NULL, 42);
ERROR HY000: mask_inner<string> UDF failed; argument 2 cannot be null
SELECT mask_inner('', 42, NULL);
ERROR HY000: mask_inner<string> UDF failed; argument 3 cannot be null
SELECT mask_inner('', NULL, 42, '');
ERROR HY000: mask_inner<string> UDF failed; argument 2 cannot be null
SELECT mask_inner('', 42, NULL, '');
ERROR HY000: mask_inner<string> UDF failed; argument 3 cannot be null
SELECT mask_inner('', 42, 42, NULL);
ERROR HY000: mask_inner<string> UDF failed; argument 4 cannot be null
# constraints checks
SELECT mask_inner('foo', -1, 0);
ERROR HY000: mask_inner<string> UDF failed; Margins can't be negative!
SELECT mask_inner('foo', 0, -1);
ERROR HY000: mask_inner<string> UDF failed; Margins can't be negative!
SELECT mask_inner('foo', 1, 1, '');
ERROR HY000: mask_inner<string> UDF failed; masking character must be of length 1
SELECT mask_inner('foo', 1, 1, ''XX);
ERROR HY000: mask_inner<string> UDF failed; masking character must be of length 1
# NULL for NULL checks
include/assert.inc [mask_inner() for the NULL primary argument should return NULL]
include/assert.inc [mask_inner() for the NULL primary argument and custom mask should return NULL]
